<% if flash[:alert]%>
  <%= js add_gritter(flash[:alert], title: "Traivan Portfolio" , sticky: false) %>
<% elsif flash[:error]%>
  <%= js add_gritter(flash[:error], title: "Traivan Portfolio" , sticky: false) %>
<% else flash[:notice]%>
  <%= js add_gritter(flash[:notice], title: "Traivan Portfolio" , sticky: false) %>
<% end %>

